<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/iv_lowExample"
        android:contentDescription="low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/iv_lowExample"
        android:layout_marginLeft="84dp"
        android:layout_marginTop="5dp"/>
	
	<TextView
        android:id="@+id/tv_lowExample"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/iv_lowExample"
        android:layout_alignBaseline="@+id/iv_lowExample"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="7dp"/>

    <ImageView
        android:id="@+id/iv_highExample"
        android:contentDescription="high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_lowExample"
        android:layout_alignTop="@+id/iv_lowExample"
        android:layout_marginLeft="25dp"/>
    
    <TextView
        android:id="@+id/tv_highExample"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/iv_highExample"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"/>

    <ImageView
        android:id="@+id/iv_popExample"
        android:contentDescription="pop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_highExample"
        android:layout_alignTop="@+id/iv_lowExample"
        android:layout_marginLeft="25dp"/>
    
    <TextView
        android:id="@+id/tv_popExample"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/iv_popExample"
        android:layout_alignTop="@+id/iv_popExample"
        android:layout_marginLeft="5dp"/>
    
    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        >
        
    </ListView>
    
</RelativeLayout>